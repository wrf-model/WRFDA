pac_cv_test_stdcall=""
case $pac_namecheck in
    X) { echo "$as_me:$LINENO: WARNING: Cannot determine Fortran naming scheme" >&5
echo "$as_me: WARNING: Cannot determine Fortran naming scheme" >&2;} ;;
    Xlower)
cat >>confdefs.h <<\_ACEOF
