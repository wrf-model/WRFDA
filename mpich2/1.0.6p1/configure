            echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
	    { echo "$as_me:$LINENO: WARNING: Compiler does not accept default optimizations" >&5
echo "$as_me: WARNING: Compiler does not accept default optimizations" >&2;}
	    MPI_DEFAULT_CXXOPTS=""
        else
            echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6
        fi
        rm -f pac_tmp_compile_stderr_opt.out pac_tmp_compile_stderr.out pac_tmp_compile_stdout_opt.out pac_tmp_compile_stdout.out
        rm -f conftest*
    fi
fi

