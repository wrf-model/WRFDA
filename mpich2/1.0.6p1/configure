    # Warn about mixed name mangling, since many of the tests will fail
    if test "$pac_cv_prog_f77_name_mangle" = "mixed" ; then
        { echo "$as_me:$LINENO: WARNING: The compiler $F77 uses mixed case names.  Fortran is monocase
