	{ echo "$as_me:$LINENO: WARNING: Timing was enabled with log option but no logging library is available.  Timing has been disabled." >&5
echo "$as_me: WARNING: Timing was enabled with log option but no logging library is available.  Timing has been disabled." >&2;}
	enable_timing=no
	timing_name=none
    fi
