echo "$as_me: error: \"$thread_impl\" is not a valid multi-thread implementation for MPICH2" >&2;}
   { (exit 1); exit 1; }; }
	;;
    esac
    ;;
    *)
    { { echo "$as_me:$LINENO: error: \"$enable_threads\" is not a valid value for --enable-threads" >&5
echo "$as_me: error: \"$enable_threads\" is not a valid value for --enable-threads" >&2;}
   { (exit 1); exit 1; }; }
