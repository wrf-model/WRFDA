test -n "$ETAGS" || ETAGS="true"

if test -n "$ETAGS" -a "$ETAGS" != true ; then
    # Check for the --langmap argument
    echo "$as_me:$LINENO: checking for etags argument to specify language" >&5
echo $ECHO_N "checking for etags argument to specify language... $ECHO_C" >&6
    cat >>conftest.c <<EOF
int footest = 0;
EOF
    if $ETAGS --langmap=c:+.i -o conftesttags conftest.c >/dev/null 2>&1 ; then
        ETAGSADD="--langmap=c:+.i"
    else
        if $ETAGS --language=c -o conftesttags conftest.c >/dev/null 2>&1 ; then
	    ETAGSADD="--language=c"
        else
            if $ETAGS --language-force=c -o conftesttags conftest.c >/dev/null 2>&1 ; then
	        ETAGSADD="--language-force=c"
            else
	        ETAGS=true
	        { echo "$as_me:$LINENO: WARNING: Could not find working etags" >&5
echo "$as_me: WARNING: Could not find working etags" >&2;}
            fi
        fi
    fi
    if test -n "$ETAGSADD" ; then
        echo "$as_me:$LINENO: result: $ETAGSADD" >&5
echo "${ECHO_T}$ETAGSADD" >&6
    fi
    rm -f conftest*
fi


