    echo "$as_me:$LINENO: checking whether install works" >&5
echo $ECHO_N "checking whether install works... $ECHO_C" >&6
    if $INSTALL conftest .conftest >/dev/null 2>&1 ; then
        installOk=yes
    else
        installOk=no
    fi
    rm -rf .conftest conftest
    echo "$as_me:$LINENO: result: $installOk" >&5
echo "${ECHO_T}$installOk" >&6
    if test "$installOk" = no ; then
        if test -n "$ac_install_sh" ; then
            INSTALL=$ac_install_sh
        else
	    { { echo "$as_me:$LINENO: error: Unable to find working install" >&5
echo "$as_me: error: Unable to find working install" >&2;}
   { (exit 1); exit 1; }; }
        fi
    fi
fi

