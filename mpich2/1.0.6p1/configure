           other_pm_names="$other_pm_names $pm_name"
       fi
    fi
    if test ! -d $use_top_srcdir/src/pm/$pm_name ; then
        { echo "$as_me:$LINENO: WARNING: $use_top_srcdir/src/pm/$pm_name does not exist. PM is unknown" >&5
echo "$as_me: WARNING: $use_top_srcdir/src/pm/$pm_name does not exist. PM is unknown" >&2;}
	hasError=yes
    elif test ! -x $use_top_srcdir/src/pm/$pm_name/configure ; then
        if test -s $use_top_srcdir/src/pm/$pm_name/configure ; then
	    { echo "$as_me:$LINENO: WARNING: The configure in $use_top_srcdir/src/pm/$pm_name exists but is not executable" >&5
echo "$as_me: WARNING: The configure in $use_top_srcdir/src/pm/$pm_name exists but is not executable" >&2;}
        else
            { echo "$as_me:$LINENO: WARNING: pm $pm_name has no configure" >&5
echo "$as_me: WARNING: pm $pm_name has no configure" >&2;}
        fi
        pm_name=""
	hasError=yes
    else
	nameserver=$MPID_PM_NAMESERVER
        if test -f $use_top_srcdir/src/pm/$pm_name/mpich2prereq ; then
	    echo sourcing $use_top_srcdir/src/pm/$pm_name/mpich2prereq
