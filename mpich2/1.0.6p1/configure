# include <inttypes.h>
#else
# if HAVE_STDINT_H
#  include <stdint.h>
# endif
#endif
#if HAVE_UNISTD_H
# include <unistd.h>
#endif"

ac_subdirs_all="$ac_subdirs_all test/mpi"
ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS RELEASE_DATE CONFIGURE_ARGS_CLEAN NUMVERSION VERSION ABIVERSION CONFIGURE_ARGUMENTS MPIFLIBNAME PMPIFLIBNAME FWRAPNAME htmldir docdir master_top_builddir master_top_srcdir DEVICE DEVICE_NAME DEVICE_ARGS INCLUDE_MPIDDEFS_H MPICXXLIBNAME logging_dir logging_name logging_subdirs debugger_dir romio_dir HAVE_ROMIO device_name MANY_PM pm_name other_pm_names pmi_name mpe_dir other_install_dirs CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT CREATESHLIB NEEDSPLIB NO_WEAK_SYM NO_WEAK_SYM_TARGET PROFILE_DEF_MPI F77 FFLAGS ac_ct_F77 FLIBS F77_NAME_MANGLE F77_OTHER_LIBS F90 F90FLAGS ac_ct_F90 F77_INCDIR FINCLUDES F77_LIBDIR_LEADER MPIFPMPI F77_IN_C_LIBS FC_SHL FC_LINKPATH_SHL F77CPP F90MODEXT F90_WORK_FILES_ARG F90MODINCFLAG F90MODINCSPEC MPIMODNAME MPICONSTMODNAME MPISIZEOFMODNAME MPIBASEMODNAME modincdir F90INC F90INCFLAG F90EXT F90CPP F90_OTHER_LIBS F90_SHL F90_LINKPATH_SHL CXX CXXFLAGS ac_ct_CXX HAVE_CXX_EXCEPTIONS CXX_DEFS CXX_SHL CXX_LINKPATH_SHL GNUC_VERSION GNUC_MINORVERSION INCLUDE_MPICXX_H FORTRAN_BINDING MPIR_CXX_BOOL MPIR_CXX_COMPLEX MPIR_CXX_DOUBLE_COMPLEX MPIR_CXX_LONG_DOUBLE_COMPLEX bindings bindings_dirs FC PERL AR RANLIB ETAGS ETAGSADD KILLALL XARGS_NODATA_OPT INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA ac_ct_RANLIB RANLIB_AFTER_INSTALL MKDIR_P MAKE VPATH SET_CFLAGS SET_MAKE BASH BUILD_BASH_SCRIPTS CPP BUILD_DLLS CC_SHL C_LINK_SHL C_LINKPATH_SHL SHLIB_EXT SHLIB_FROM_LO SHLIB_INSTALL ENABLE_SHLIB pac_prog LIBTOOL BUILD_TVDLL CC_SHL_DBG C_LINK_SHL_DBG nameserv_name DOCTEXT DOCTEXTSTYLE MAKE_DEPEND_C GCC EGREP INT16_T INT32_T INT64_T MPI_CHAR MPI_UNSIGNED_CHAR MPI_SHORT MPI_UNSIGNED_SHORT MPI_INT MPI_UNSIGNED_INT MPI_LONG MP