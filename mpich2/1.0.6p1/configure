    # ctype is a valid C type which we can use to declare a C version of
    # this item
    ctype=`echo $type | sed 's/_/ /'`
    eval len=\$ac_cv_sizeof_$type
    if test -n "$len" ; then
    case $len in
    1) if test "$MPI_INTEGER1" = "MPI_DATATYPE_NULL" ; then
