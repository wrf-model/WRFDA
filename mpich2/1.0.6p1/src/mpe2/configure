ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS top_srcdir_abs VPATH MAKE MKDIR CPRP RM MV includebuild_dir libbuild_dir binbuild_dir sbinbuild_dir etcbuild_dir MPI_CC MPI_F77 MPI_INC MPI_LIBS MPERUN htmldir docdir CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT F77 FFLAGS ac_ct_F77 FLIBS F77_NAME_MANGLE F77_OTHER_LIBS F77_LIBDIR_LEADER F77_INCDIR CPP LN_S INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA RANLIB ac_ct_RANLIB RANLIB_AFTER_INSTALL MPI_IMPL MPE_LIBNAME MPE_BUILD_FORTRAN2C CLINKER MPE_CFLAGS MPE_CLINKER FLINKER MPE_FFLAGS MPE_FLINKER subdirs LIBOBJS LTLIBOBJS'
ac_subst_files=''

